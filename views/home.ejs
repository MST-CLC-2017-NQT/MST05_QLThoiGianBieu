<!doctype html>
<html>

<head>
    <title>Product Cart</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7"
        crossorigin="anonymous">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css">
    <link rel='stylesheet' href='public/stylesheets/style.css' />
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <style>
        body {
            padding-top: 0px;
        }
    </style>
    <script>
        function logout() {
            var _csrf = document.getElementById('_csrf').value;
            $.ajax({
                type: 'POST',
                url: '/user/logout',
                headers: { 'XSRF-TOKEN': _csrf },
                error: function (res) {
                    btn.parentNode.parentNode.className = "";
                },
                success: function (res) {
                }
            });
        }
    </script>
</head>

<body>
    <!-- Header -->
    <%- include('partials/header') %>
        <div class="container">
            <input type="hidden" name="_csrf" id="_csrf" value="<%= csrfToken %>">
            <div class="jumbotron text-center">
                <h1>
                    <span class="fa fa-lock"></span> QUAN LY THOI GIAN BIEU</h1>

                <a href="/event/addEvent" class="btn btn-default">
                    <span class="fa fa-user"></span> Local</a>

            </div>
            <!-- Footer -->
            <% include partials/footer.ejs%>

        </div>

</body>

</html>